{
  "extends": [
    "../.eslintrc.json"
  ],
  "overrides": [
    {
      "files": [
        "*.js"
      ],
      "parser": "babel-eslint",
      "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
      },
      "settings": {
        "import/resolver": {
          "parcel": {
            "rootDir": "src",
            "extensions": [
              ".js"
            ]
          }
        }
      }
    }
  ]
}
